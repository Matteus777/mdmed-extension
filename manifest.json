{
  "manifest_version": 3,
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "hello.html",
    "default_icon": "hello_extensions.png"
  },
  "host_permissions": [
    "*://*.mdmed.clinic/*",
    "<all_urls>",
    "http://localhost/*"
  ],
  "permissions": [
    "webRequest",
    "activeTab",
    "tabs",
    "scripting"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "dialog.html"
      ],
      "matches": [
        "*://*.mdmed.clinic/*"
      ]
    }
  ],
  "background": {
    "service_worker": "injectable.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://mdmed.clinic/main/medical-history/view/*"
      ],
      "run_at": "document_end",

      "js": [
        "dialog.js"
      ]
    }
  ]
}